<view class="container"><view><text>{{"服务名称: "+service.name}}</text><text>{{"价格: "+price}}</text></view><view><text>卫生程度:</text><picker range="{{['简单','中等','困难']}}" data-event-opts="{{[['change',[['onHygieneChange',['$event']]]]]}}" bindchange="__e"></picker></view><view><checkbox-group data-event-opts="{{[['change',[['onMemberChange',['$event']]]]]}}" bindchange="__e"><label><checkbox value="member"></checkbox>会员</label></checkbox-group></view><view><input placeholder="填写地址" data-event-opts="{{[['input',[['__set_model',['','address','$event',[]]]]]]}}" value="{{address}}" bindinput="__e"/></view><view><picker mode="date" data-event-opts="{{[['change',[['onDateChange',['$event']]]]]}}" bindchange="__e"></picker></view><view><input placeholder="填写备注" data-event-opts="{{[['input',[['__set_model',['','remark','$event',[]]]]]]}}" value="{{remark}}" bindinput="__e"/></view><button data-event-opts="{{[['tap',[['submitOrder',['$event']]]]]}}" bindtap="__e">提交订单</button></view>